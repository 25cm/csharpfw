                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>select
	yhdt.YoshiHanbaiChumonNo as HakkoNo, --発行No
	gm.GyoshaCd, --業者コード
	gm.GyoshaNm, --宛名
	gm.GyoshaAdr, --住所
	gm.GyoshaTelNo, --TEL
	gm.GyoshaZipCd, --郵便番号
	gm.GyoshaFaxNo, --FAX
	yhdt.YoshiHanbaiYoshiCd as Hinban, --品番
	ym.YoshiNm as HinNm, --品名
	'' as Tani, --単位
	case
		when yhdt.YoshiHanbaiSetSuryo &gt; 0 then yhdt.YoshiHanbaiSetSuryo
		when yhdt.YoshiHanbaiSuryo &gt; 0 then yhdt.YoshiHanbaiSuryo
	end as SuiRyo, --数量
	case
		when yhdt.YoshiHanbaiShohizei &gt; 0 then '1'
		when yhdt.YoshiHanbaiShohizei = 0 then '0'
	end as ShouhizeiUmu, --消費税有無
	case
		when yhdt.YoshiHanbaiSetSuryo &gt; 0 then yhdt.YoshiHanbaiSetKakaku
		when yhdt.YoshiHanbaiSuryo &gt; 0 then yhdt.YoshiHanbaiUp
	end as Tanka, --単価
	yhdt.YoshiHanbaiKakaku as Kingaku, --金額
	'' as Bikou, --備考
              '' as TaniNm,
	yhdt.YoshiHanbaiGaku as HanbaiGaku --販売額
from YoshiHanbaiHdrTbl yhht
left outer join YoshiHanbaiDtlTbl yhdt
	on yhht.YoshiHanbaiChumonNo = yhdt.YoshiHanbaiChumonNo
left outer join YoshiMst ym
	on yhdt.YoshiHanbaiYoshiCd = ym.YoshiCd
left outer join GyoshaMst gm
	on yhht.YoshiHanbaisakiGyoshaCd = gm.GyoshaCd
order by yhdt.YoshiHanbaiYoshiCd</CommandText>
